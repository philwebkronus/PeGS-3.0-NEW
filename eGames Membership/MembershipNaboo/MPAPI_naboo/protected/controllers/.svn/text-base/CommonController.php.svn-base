<?php

/**
 * Get the return output of the MPapi
 * @return array
 * @date 12-03-2015
 * @author taalcantara
 */

class CommonController {
    
    public static function retMsgRegisterMemberNaboo($module, $errorCode, $transMsg) {
        $module = 'RegisterMemberNaboo';
        return array($module => array('ErrorCode' => $errorCode, 'ReturnMessage' => $transMsg));
    }

    public static function retMsgReloadNaboo($module, $errorCode, $transMsg) {
        $module = 'ReloadNaboo';
        return array($module => array('ErrorCode' => $errorCode, 'ReturnMessage' => $transMsg));
    }
  
}
?>
